                        </cal:real>
                        <cal:real>
                            <gco:Real>2</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>3</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>4</gco:Real>
                        </cal:real>
                    </cal:OrderedReal>
                </cal:amplitude>
                <cal:phase>
                    <cal:OrderedReal>
                        <cal:real>
                            <gco:Real>1</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>2</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>3</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>4</gco:Real>
                        </cal:real>
                    </cal:OrderedReal>
                </cal:phase>
            </cal:pattern>
        </cal:CA_AntennaPattern>
    </cal:crossPolAntennaPattern>
    <cal:TBWithinTheScene>
        <gco:Real>100</gco:Real>
    </cal:TBWithinTheScene>
</cal:CA_AntennaPatternCalibration>
