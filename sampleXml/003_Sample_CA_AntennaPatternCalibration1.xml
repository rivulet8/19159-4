    </cal:spillover>
    <cal:satelliteAttitude/>
    <cal:satelliteOrbitalHeight>
        <gco:Real>100</gco:Real>
    </cal:satelliteOrbitalHeight>
    <cal:beamwidth>
        <gco:Angle uom="degrees">80</gco:Angle>
    </cal:beamwidth>
    <cal:mainBeamEfficiency>
        <gco:Real>0.5</gco:Real>
    </cal:mainBeamEfficiency>
    <cal:coPolAntennaPattern>
        <cal:CA_AntennaPattern>
            <cal:patternElevationAngle>
                <cal:OrderedAngle>
                    <cal:angle>
                        <gco:Angle uom="degree">10</gco:Angle>
                    </cal:angle>
                    <cal:angle>
                        <gco:Angle uom="degree">20</gco:Angle>
                    </cal:angle>
                </cal:OrderedAngle>
            </cal:patternElevationAngle>
