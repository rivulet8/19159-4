                        </cal:angle>
                        <cal:angle>
                            <gco:Angle uom="degree">40</gco:Angle>
                        </cal:angle>
                        <cal:angle>
                            <gco:Angle uom="degree">50</gco:Angle>
                        </cal:angle>
                    </cal:OrderedAngle>
                </cal:azimuthAngle>
                <cal:amplitude>
                    <cal:OrderedReal>
                        <cal:real>
                            <gco:Real>1</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>2</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>3</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>4</gco:Real>
                        </cal:real>
                    </cal:OrderedReal>
                </cal:amplitude>
                <cal:phase>
                    <cal:OrderedReal>
                        <cal:real>
                            <gco:Real>1</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>2</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>3</gco:Real>
                        </cal:real>
                        <cal:real>
                            <gco:Real>4</gco:Real>
                        </cal:real>
                    </cal:OrderedReal>
                </cal:phase>
            </cal:pattern>
