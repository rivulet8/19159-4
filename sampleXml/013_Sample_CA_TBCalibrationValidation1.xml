<?xml version="1.0" encoding="UTF-8"?>
<cal:CA_TBCalibrationValidation xmlns:cal="https://schemas.isotc211.org/19159/-4/cal/1.0"
    xmlns:gco="https://schemas.isotc211.org/19103/-/gco/1.2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://schemas.isotc211.org/19159/-4/cal/1.0 file:/F:/Code/ISOReview-19159-4/schemas/schemas.isotc211.org/19159/-4/cal/1.0.0/cal.xsd">
    <cal:TBWithinTheScene>
        <gco:Real>1</gco:Real>
    </cal:TBWithinTheScene>
    <cal:payloadOperatingStatus>
        <cal:CA_PayloadOperatingStatus codeList="codelist" codeListValue="code value">code label</cal:CA_PayloadOperatingStatus>
    </cal:payloadOperatingStatus>
    <cal:TBTrueValue>
        <cal:CA_AbsoluteCalibrationTrueValue>
            <cal:TBTrueValue>
                <gco:Real>1</gco:Real>
            </cal:TBTrueValue>
            <cal:uncertainty>
                <gco:Real>2</gco:Real>
            </cal:uncertainty>
            <cal:atmosphereProfile>
                <cal:CA_AtmosphereProfile>
                    <cal:atmosphereTemperature>
                        <cal:OrderedReal>
                            <cal:real>
                                <gco:Real>1</gco:Real>
                            </cal:real>
                            <cal:real>
                                <gco:Real>2</gco:Real>
                            </cal:real>
                        </cal:OrderedReal>
                    </cal:atmosphereTemperature>
                    <cal:atmosphereHumidity>
                        <cal:OrderedReal>
                            <cal:real>
                                <gco:Real>1</gco:Real>
                            </cal:real>
                            <cal:real>
                                <gco:Real>2</gco:Real>
                            </cal:real>
                        </cal:OrderedReal>
                    </cal:atmosphereHumidity>
                    <cal:atmospherePressure>
                        <cal:OrderedReal>
                            <cal:real>
                                <gco:Real>1</gco:Real>
                            </cal:real>
                            <cal:real>
                                <gco:Real>2</gco:Real>
                            </cal:real>
                        </cal:OrderedReal>
                    </cal:atmospherePressure>
                    <cal:atmosphereLiquidWater>
                        <cal:OrderedReal>
                            <cal:real>
                                <gco:Real>1</gco:Real>
                            </cal:real>
                            <cal:real>
                                <gco:Real>2</gco:Real>
                            </cal:real>
                        </cal:OrderedReal>
                    </cal:atmosphereLiquidWater>
                    <cal:cloudCover>
                        <gco:Real>1</gco:Real>
                    </cal:cloudCover>
                </cal:CA_AtmosphereProfile>
            </cal:atmosphereProfile>
            <cal:oceanSurface>
                <cal:CA_OceanSurface>
                    <cal:oceanSurfaceTemperature>
                        <gco:Real>1</gco:Real>
                    </cal:oceanSurfaceTemperature>
                    <cal:oceanSurfaceSalinity>
                        <gco:Real>2</gco:Real>
                    </cal:oceanSurfaceSalinity>
                    <cal:oceanSurfaceRoughness>
                        <gco:Real>3</gco:Real>
                    </cal:oceanSurfaceRoughness>
                    <cal:oceanWindSpeed>
                        <gco:Real>4</gco:Real>
                    </cal:oceanWindSpeed>
                    <cal:oceanWindDirection>
                        <gco:Real>5</gco:Real>
                    </cal:oceanWindDirection>
                    <cal:seaIceConcentration>
                        <gco:Real>6</gco:Real>
                    </cal:seaIceConcentration>
                </cal:CA_OceanSurface>
            </cal:oceanSurface>
            <cal:landSurface>
                <cal:CA_LandSurface>
                    <cal:soilMoisture>
                        <gco:Real>1</gco:Real>
                    </cal:soilMoisture>
                    <cal:landSurfaceTemperature>
                        <gco:Real>2</gco:Real>
                    </cal:landSurfaceTemperature>
                    <cal:landCoverType>
                        <cal:CA_LandCoverType codeList="codelist" codeListValue="code value">code label</cal:CA_LandCoverType>
                    </cal:landCoverType>
                </cal:CA_LandSurface>
            </cal:landSurface>
            <cal:radiativeTransferModel>
                <cal:CA_RadiativeTransferModel>
                    <cal:atmosphereAbsorption>
                        <gco:Real>1</gco:Real>
                    </cal:atmosphereAbsorption>
                    <cal:oceanEmissivity>
                        <gco:Real>2</gco:Real>
                    </cal:oceanEmissivity>
                    <cal:oceanReflectivity>
                        <gco:Real>3</gco:Real>
                    </cal:oceanReflectivity>
                    <cal:landSurfaceEmissivity>
                        <gco:Real>4</gco:Real>
                    </cal:landSurfaceEmissivity>
                </cal:CA_RadiativeTransferModel>
            </cal:radiativeTransferModel>
            <cal:geophysicsQualifyFlags>
                <cal:CA_GeophysicsQualifyFlags>
                    <cal:oceanLandFlag>
                        <gco:Integer>1</gco:Integer>
                    </cal:oceanLandFlag>
                    <cal:cloudFlag>
                        <gco:Integer>1</gco:Integer>
                    </cal:cloudFlag>
                    <cal:rainFlag>
                        <gco:Integer>0</gco:Integer>
                    </cal:rainFlag>
                    <cal:seaIceFlag>
                        <gco:Integer>0</gco:Integer>
                    </cal:seaIceFlag>
                </cal:CA_GeophysicsQualifyFlags>
            </cal:geophysicsQualifyFlags>
        </cal:CA_AbsoluteCalibrationTrueValue>
    </cal:TBTrueValue>
    <cal:consistencyThreshold>
        <gco:Real>1</gco:Real>
    </cal:consistencyThreshold>
    <cal:TBCalibrationBias>
        <gco:Real>1</gco:Real>
    </cal:TBCalibrationBias>
    <cal:TBCalibrationStandardDeviation>
        <gco:Real>1</gco:Real>
    </cal:TBCalibrationStandardDeviation>
    <cal:calibrationParameterAdjustments>
        <cal:CA_CalibrationParameterAdjustments>
            <cal:hotTargetBTBias>
                <gco:Real>1</gco:Real>
            </cal:hotTargetBTBias>
            <cal:coldTargetBTBias>
                <gco:Real>2</gco:Real>
            </cal:coldTargetBTBias>
            <cal:nonlinearityCoeffecientCorrection>
                <gco:Real>3</gco:Real>
            </cal:nonlinearityCoeffecientCorrection>
            <cal:antennaPatternCorrectionCoefficient>
                <cal:OrderedReal>
                    <cal:real><gco:Real>1</gco:Real></cal:real>
                    <cal:real><gco:Real>2</gco:Real></cal:real>
                </cal:OrderedReal>
            </cal:antennaPatternCorrectionCoefficient>
        </cal:CA_CalibrationParameterAdjustments>
    </cal:calibrationParameterAdjustments>
    <cal:reCalibratedTB>
        <gco:Real>100</gco:Real>
    </cal:reCalibratedTB>
    <cal:calibrationResults/>
</cal:CA_TBCalibrationValidation>
